#!/usr/bin/env node

'use strict';

var path       = require('path'),
    parse      = require('../lib/argParser'),
    Venus      = require('../Venus'),
    loadConfig = require('../lib/configLoader'),
    merge      = require('deepmerge');

var venus, args, config;

args   = parse(process.argv);
config = loadConfig(args.config);

new Venus(merge(config, args)).start();

