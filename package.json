{
  "name": "venus",
  "main": "Venus.js",
  "description": "Execution environment for JavaScript unit testing",
  "version": "2.3.9",
  "keywords": [
    "testing"
  ],
  "homepage": "https://github.com/linkedin/venus.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/linkedin/venus.js.git"
  },
  "bugs": {
    "url": "https://github.com/linkedin/venus.js/issues"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "bin": {
    "venus": "./bin/venus"
  },
  "license": "Apache 2.0",
  "dependencies": {
    "async": "2.6.2",
    "chokidar": "3.0.0",
    "cli-prompt": "0.6.0",
    "colors": "1.3.3",
    "commander": "2.20.0",
    "consolidate": "0.15.1",
    "deferred": "0.7.10",
    "dustjs-linkedin": "2.7.5",
    "expect.js": "~0.3.1",
    "express": "4.16.4",
    "flavored-path": "0.0.8",
    "fs-tools": "0.5.0",
    "i18n": "0.8.3",
    "istanbul": "0.4.5",
    "json5": "2.1.0",
    "mkdirp": "0.5.1",
    "graceful-fs": "4.1.15",
    "phantomjs": "2.1.7",
    "portscanner": "2.2.0",
    "rmrf": "2.0.0",
    "sauce-connect-launcher": "1.2.6",
    "saucelabs": "2.1.8",
    "selenium-webdriver": "3.6.0",
    "socket.io": "2.2.0",
    "socket.io-client": "2.2.0",
    "underscore": "1.9.1",
    "underscore.string": "2.2.0rc",
    "useragent-parser": "0.1.1",
    "when": "3.7.8",
    "winston": "3.2.1",
    "wrench": "1.5.9"
  },
  "devDependencies": {
    "grunt": "1.0.4",
    "grunt-release": "0.14.0",
    "load-grunt-tasks": "4.0.0",
    "mocha": "6.1.4",
    "sinon": "7.3.2"
  },
  "os": [
    "!win32"
  ],
  "preferGlobal": true,
  "scripts": {
    "test": "./node_modules/.bin/mocha test/unit/node/test_suite.js && ./bin/venus run -t test/unit/client -n",
    "int-test": "./node_modules/.bin/mocha test/integration"
  }
}
