{
    "env": {
        "browser": false,
        "node": true,
        "amd": false
    },

    "rules": {

        "no-alert": 1,
        "no-caller": 1,
        "no-bitwise": 0,
        "no-console": 1,
        "no-debugger": 1,
        "no-else-return": 0,
        "no-empty": 1,
        "no-eq-null": 0,
        "no-eval": 1,
        "no-floating-decimal": 0,
        "no-implied-eval": 1,
        "no-with": 1,
        "no-fallthrough": 1,
        "no-unreachable": 1,
        "no-undef-init": 1,
        "no-octal": 1,
        "no-new-wrappers": 1,
        "no-new": 1,
        "no-new-func": 1,
        "no-plusplus": 0,
        "no-ternary": 0,
        "no-self-compare": 0,
        "no-sync": 0,
        "no-mixed-requires": [0, false],

        "smarter-eqeqeq": 0,
        "brace-style": 0,
        "camelcase": 1,
        "curly": 1,
        "dot-notation": 1,
        "eqeqeq": 1,
        "new-parens": 1,
        "guard-for-in": 0,
        "radix": 0,
        "new-cap": 1,
        "one-var": 0,
        "quote-props": 0,
        "semi": 1,
        "use-isnan": 1,
        "quotes": [1, "single"],
        "max-depth": [0, 4],
        "max-params": [0, 3],
        "max-statements": [0, 10],
        "complexity": [0, 11],
        "wrap-iife": 0,
        "consistent-this": [0, "that"],
        "max-len": [0, 80, 4]
    }
}
